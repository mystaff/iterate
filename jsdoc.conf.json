{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": [
      "index.js", "./iterate/", "./helpers/", "./logic", "./debug"
    ],
    "exclude":[],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "./node_modules/docdash",
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
		"default": {
			"outputSourceFiles": true,
      "staticFiles": {
        "include": ["ui/"]
      }
	  },
    "footerText": "Iterate Library Documentation &copy; TimeDoctor LLC.",
    "useCollapsibles": true
  },
  "docdash": {
    "search": true,
    "typedefs": true,
    "wrap": true,
    "static": true,
    "instance": true,
    "scripts": [
      "styles/custom.css",
      "scripts/custom.js"
    ]
  }
}
